            <add baseAddress="http://localhost:8080/MyService.svc" />
            <!--<add baseAddress="net.tcp://localhost:8090/" />-->
          </baseAddresses>
        </host>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="MexGet" >
          <serviceMetadata httpGetEnabled="true" />
        </behavior>
      </serviceBehaviors>
    </behaviors>

  </system.serviceModel>

